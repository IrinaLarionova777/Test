<Method name="Head">
<Description>
Display shandard header lines.
This will automatically include the standard javascript and style sheets.</Description>
<Final>0</Final>
<ClassMethod>1</ClassMethod>
<CodeMode>code</CodeMode>
<FormalSpec>page:%String=%request.PageName</FormalSpec>
<Private>0</Private>
<SqlProc>0</SqlProc>
<Implementation><![CDATA[
 //
 n itm,msg
 #define QuoteJS(%n) "'"_$ZCVT(%n,"O","JS")_"'"
 //
 //
 ; Nasty hack as ServerReDirect will lose %request items
 ; Restore %request after a redirect
 d ##Class(websys.Conversions).LoadRequest()
 // Do this first *this needs to come after LoadRequest - or we won;t have the stuff !
 d ##Class(websys.Conversions).CommonReload()
 ;
 ; moved to websys.csp
 n id,page
 ;s id=##Class(websys.Conversions).SaveReload()
 ;s id=$g(%session.Data("TRELOAD"))
 s id=$g(TRELOADNEW)
 s page=$g(TRELOADPAGE)
 i page="" s page=$g(^TMP("reload",%session.SessionId,id,"RequestPageName"))
 i page="" s page=%request.PageName
 &html<<SCRIPT SRC="/csp/broker/cspbroker.js"></SCRIPT><SCRIPT SRC="/csp/broker/cspxmlhttp.js"></SCRIPT>
 <SCRIPT SRC="../scripts/websys.js"></SCRIPT>
 <LINK REL="stylesheet" TYPE="text/css" HREF="../scripts/websys.css"></LINK>>
 i $g(%session.Data("LOGON.STYLESHEET"))'="" {
 w "<LINK REL=""stylesheet"" TYPE=""text/css"" HREF=""",$g(%session.Data("LOGON.STYLESHEET")),"""></LINK>",!
 }
 &html<
 <SCRIPT language='javascript'>
 var TRELOADPAGE='#(page)#';
 var TRELOADID='#(id)#';
 function treload(csppage) {
 window.location.href= "#(page)#?TRELOADID=#(id)#&TRELOAD=1";
 }
 </SCRIPT>
 >
 ;
 k t
 n js,c,m
 s js=""
 d ##Class(websys.Translation).Load("COMP",$g(%session.Data("LOGON.LANGID")),550)
 d ##Class(websys.Translation).LoadContext($g(%session.Data("LOGON.LANGID")),550,$g(%request.Data("CONTEXT",1)))
 ;
 ; Initialise the session variables for use in script
 ;
 s c="" f  s c=$o(t(c)) q:c=""  s t(c)=$p(t(c),"^")
 w "<script language=javascript>",!
 w "var t=new Array();",!
 w "t['XMISSING']="_$$$QuoteJS($g(t("XMISSING"),"is a required field but has not been entered"))_";",!
 w "t['XDATE']="_$$$QuoteJS($g(t("XDATE"),"is a date field but does not have a valid date entered"))_";",!
 w "t['XTIME']="_$$$QuoteJS($g(t("XTIME"),"is a time field but does not have a valid time entered"))_";",!
 w "t['XNUMBER']="_$$$QuoteJS($g(t("XNUMBER"),"is a numeric field but does not have a valid number entered"))_";",!
 w "t['XINVALID']="_$$$QuoteJS($g(t("XINVALID"),"does not have a valid entry"))_";",!
 w "t['XLAYOUTERR']="_$$$QuoteJS($g(t("XLAYOUTERR"),"'The TRAK Layout Editor is not functioning or has not been installed.\n\n 1. Please check that your browser security settings allow you to initialize and script activeX controls.\n 2. Please check that the TRAK Layout Editor has been installed.'"))_";",!
 w "t['XLOCKED']="_$$$QuoteJS($g(t("XLOCKED"),"Record is locked by another user."))_";",!
 w "t['XLOCKEDCT']="_$$$QuoteJS($g(t("XLOCKEDCT"),"Code table updates are currently disabled."))_";",!
 w "t['XNOTCT']="_$$$QuoteJS($g(t("XNOTCT"),"You must connect to the code table server to update code tables."))_";",!
 w "t['XLOCKEDMT']="_$$$QuoteJS($g(t("LOCKEDMT"),"Record is locked by MEDTRAK."))_";",!
 w "t['XDAYS']="_$$$QuoteJS($g(t("XDAYS"),"Sun,Mon,Tue,Wed,Thu,Fri,Sat"))_";",!
 w "t['XMONTHS']="_$$$QuoteJS($g(t("XMONTHS"),"January,February,March,April,May,June,July,August,September,October,November,December"))_";",!
 w "t['XUNSAVED']="_$$$QuoteJS($g(t("XUNSAVED"),"There are unsaved changes on this page."))_";",!
 w "t['XSORTMAXROWS']="_$$$QuoteJS($g(t("XSORTMAXROWS"),"Rows retrieved for sorting exceeds maximum specified in system configuration. Sort terminated."))_";",!

 w "var session=new Array();",!
 w "session['LOGON.SITECODE']="_$$$QuoteJS($g(%session.Data("LOGON.SITECODE")))_";",!
 w "session['LOGON.USERID']="_$$$QuoteJS($g(%session.Data("LOGON.USERID")))_";",!
 w "session['LOGON.USERCODE']="_$$$QuoteJS($g(%session.Data("LOGON.USERCODE")))_";",!
 w "session['LOGON.USERNAME']="_$$$QuoteJS($g(%session.Data("LOGON.USERNAME")))_";",!
 w "session['LOGON.GROUPID']="_$$$QuoteJS($g(%session.Data("LOGON.GROUPID")))_";",!
 w "session['LOGON.GROUPDESC']="_$$$QuoteJS($g(%session.Data("LOGON.GROUPDESC")))_";",!
 w "session['LOGON.LANGID']="_$$$QuoteJS($g(%session.Data("LOGON.LANGID")))_";",!
 w "session['XMONTHSSHORT']="_$$$QuoteJS($g(%session.Data("XMONTHSSHORT"),"Jan,Feb,Mar,Apr,May,Jun,Jul,Aug,Sep,Oct,Nov,Dec"))_";",!
 w "session['CONTEXT']="_$$$QuoteJS($g(%request.Data("CONTEXT",1)))_";",!
 w "window.status=session['LOGON.USERCODE'];"


	i +$g(%session.Data("MESSAGE","N")) {
		n msgtxt
		s (msg,msgtxt)=""
		f  {
			s msg=$o(%session.Data("MESSAGE","N",msg))
			i msg="" q
			s itm=$g(%session.Data("MESSAGE","N",msg))
			; only put a colon in the message if we have a header
			i $zcvt($lg(itm,2),"O","JS")'="" {
				; put a newline if not the first mssage in the alert
				i msg>1 s msgtxt=msgtxt_" \n\n"
			;	s msgtxt=msgtxt_$zcvt($lg(itm,2),"O","JS")_": "
			}
			s msgtxt=msgtxt_$zcvt($lg(itm,3),"O","JS")
		}		
		w "	alert('"_msgtxt_"');",!				
	}
 k %session.Data("MESSAGE","N"),%session.Data("MESSAGE","NX")
 w "</script>",!

 //&html<<script language=javascript>
 //var t=new Array();
 //<%=js%>
 //var session=new Array();
 //<%=js2%>
 //</script>>
 ;
 q
]]></Implementation>
</Method>

